{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "augment-dist": "cp {package.json,yarn.lock,README.md} ./dist",
    "prepublishOnly": "tsdx lint && tsdx test && tsdx build && yarn augment-dist"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@99s/dgraph-new",
  "author": "Ivan P",
  "module": "dist/dgraph-new.esm.js",
  "size-limit": [
    {
      "path": "dist/dgraph-new.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dgraph-new.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  }
}
